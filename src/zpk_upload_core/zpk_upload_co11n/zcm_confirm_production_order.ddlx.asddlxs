@Metadata.layer: #CORE
@UI:{
    headerInfo: { typeName: 'Manage File',
                  typeNamePlural: 'Upload Confirmation Production Order',
                  title: { value: 'Filename' }
    }
}

@UI.lineItem: [{criticality: 'Criticality'}]
annotate view ZCM_CONFIRM_PRODUCTION_ORDER with
{
  @UI.facet          : [
    {
     id                 : 'General',
     purpose            : #STANDARD,
     position           : 10,
     isPartOfPreview    : true,
     label              : 'General',
     type               : #COLLECTION,
     targetQualifier    : 'General'
     },
     {
     id                 : 'BasicInfo',
     purpose            : #STANDARD,
     parentId           : 'General',
     position           : 10,
     isPartOfPreview    : true,
     label              : 'Basic Info',
     type               : #FIELDGROUP_REFERENCE,
     targetQualifier    : 'QFBasicInfo'
     },
     {
     id                 : 'Items',
     purpose            : #STANDARD,
     type               : #LINEITEM_REFERENCE,
     label              : 'Data File',
     position           : 20,
     targetElement      : '_DataFile'
     }
     ]

  @UI.lineItem       : [{ position: 10 }]
  @UI.identification : [{ position: 10 }]
  @UI.fieldGroup     : [ { type: #STANDARD,  position: 10 ,  qualifier: 'QFBasicInfo'  }]
  @UI.hidden: true
  Uuid;

  @UI.lineItem       : [{ position: 20 }]
  @UI.identification : [{ position: 20 }]
  @UI.fieldGroup     : [ { type: #STANDARD,  position: 20 ,  qualifier: 'QFBasicInfo'  }]
  @EndUserText.label: 'No. '
  Zcount;

  @UI.selectionField : [{ position: 30 }]
  @UI.lineItem       : [{ position: 30, label: 'Status' , criticality: 'Criticality' }]
  @UI.identification : [{ position: 30 }]
  @UI.fieldGroup     : [ { type: #STANDARD,  position: 30 ,  qualifier: 'QFBasicInfo'  }]
  @UI.dataPoint : { title: 'Status', criticality: 'Criticality' }
  Status;
  
  @Consumption.filter.hidden: true
  Criticality;

  @UI.hidden: true
  OverallStatusText;

  @UI.lineItem       : [{ position: 40 }]
  @UI.identification : [{ position: 40 }]
  @Consumption.filter.hidden: true
  Attachment;

  @UI.lineItem       : [{ position: 50 }]
  @UI.identification : [{ position: 50 }]
  @Consumption.filter.hidden: true
  Mimetype;

  @UI.selectionField : [{ position: 60 }]
  @UI.lineItem       : [{ position: 60 }]
  @UI.identification : [{ position: 60 }]
  @UI.fieldGroup     : [ { type: #STANDARD,  position: 60 ,  qualifier: 'QFBasicInfo'  }]
  Filename;

  @UI.selectionField : [{ position: 70 }]
  @UI.lineItem       : [{ position: 70 }]
  @UI.identification : [{ position: 70 }]
  @UI.fieldGroup     : [ { type: #STANDARD,  position: 70 ,  qualifier: 'QFBasicInfo'  }]
  @EndUserText.label: 'Total line of File'
  @Consumption.filter.hidden: true
  Countline;

  @UI.selectionField : [{ position: 80 }]
  @UI.lineItem       : [{ position: 80 }]
  @UI.identification : [{ position: 80 }]
  @UI.fieldGroup     : [ { type: #STANDARD,  position: 80 ,  qualifier: 'QFBasicInfo'  }]
  Createdbyuser;

  @UI.selectionField : [{ position: 90 }]
  @UI.lineItem       : [{ position: 90 }]
  @UI.identification : [{ position: 90 }]
  @UI.fieldGroup     : [ { type: #STANDARD,  position: 90 ,  qualifier: 'QFBasicInfo'  }]
  Createddate;

  @UI.selectionField : [{ position: 100 }]
  @UI.lineItem       : [{ position: 100 }]
  @UI.identification : [{ position: 100 }]
  @UI.fieldGroup     : [ { type: #STANDARD,  position: 100 ,  qualifier: 'QFBasicInfo'  }]
  Changedbyuser;

  @UI.selectionField : [{ position: 110 }]
  @UI.lineItem       : [{ position: 110 }]
  @UI.identification : [{ position: 110 }]
  @UI.fieldGroup     : [ { type: #STANDARD,  position: 110 ,  qualifier: 'QFBasicInfo'  }]
  Changeddate;

  /* Associations */
  //    _DataFile;
  //    _OverallStatus;

}