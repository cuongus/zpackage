projection;
strict ( 2 );
use draft;

define behavior for ZC_USER_UPLOAD_FI alias file
{
  use create;
  use update;
  use delete;

  use action Edit;
  use action Activate;
  use action Discard;
  use action Resume;
  use action Prepare;

  use association _previewData { create; with draft; }
}

define behavior for ZC_UUID_DATA_FILE1 alias previewData
{
  use update;
  use delete;

  use association _File { with draft; }
}